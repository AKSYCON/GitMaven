pipeline {
  agent any
  environment {
    CI = true
    ARTIFACTORY_ACCESS_TOKEN = credentials('jFrog-token')
  }
  stages {
    stage('Build') {
      steps {
        bat 'mvn clean install'
      }
    }
    stage('Upload to Artifactory') {
      steps {
          bat 'jf rt upload --url http://192.168.1.230:8082/artifactory/ --access-token ${ARTIFACTORY_ACCESS_TOKEN} com\jenkinns\JenkinsProj\0.0.1-SNAPSHOT\JenkinsProj-0.0.1-SNAPSHOT.war java-web-app/'
      }
    }
  }
}
