pipeline {
  agent any
  environment {
    CI = true
    ARTIFACTORY_ACCESS_TOKEN = credentials('jFrog-token')
  }
  stages {
    stage('Build') {
      steps {
        bat 'mvn clean install'
      }
    }
    stage('Upload to Artifactory') {
      steps {
          sh 'jfrog rt upload --url http://127.0.0.1:8082/artifactory/ --access-token ${ARTIFACTORY_ACCESS_TOKEN} target/demo-0.0.1-SNAPSHOT.jar java-web-app/'
      }
    }
  }
}
